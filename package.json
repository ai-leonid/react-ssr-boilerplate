{
  "name": "BoilerplateExample",
  "version": "1.0.0",
  "description": "Site sources of example boilerplate",
  "main": "index.js",
  "scripts": {
    "remove": "./node_modules/.bin/rimraf ./dist/*js* && ./node_modules/.bin/rimraf ./dist/public/*",
    "server": "node ./dist/server.js",
    "server-assets": "node ./dist/server.js --assets",
    "build-server": "./node_modules/.bin/webpack --config ./webpack/config.server.js",
    "build-client": "./node_modules/.bin/webpack --config ./webpack/config.client.js",
    "build": "npm run remove && ./node_modules/.bin/npm-run-all --parallel build-client build-server",
    "hot-server": "./node_modules/.bin/webpack --config  ./webpack/config.server.js --isHot",
    "hot-client": "./node_modules/.bin/webpack-dev-server --config ./webpack/config.client.js --isHot",
    "start": "npm run remove && ./node_modules/.bin/npm-run-all --parallel hot-server hot-client"
  },
